<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.cyh.b1.notice.NoticeFilesMapper">
  
  <insert id="noticeFilesInsert" parameterType="com.cyh.b1.notice.NoticeFilesVO">
 		insert into noticeFiles values(null,#{num},#{fname},#{oname})
 	
 </insert>
 	
 <select id="noticeFilesSelect" parameterType="com.cyh.b1.notice.NoticeFilesVO" resultType="com.cyh.b1.notice.NoticeFilesVO">
 			select * from noticeFiles where fnum= #{fnum}
 </select> 
  
  
  </mapper>